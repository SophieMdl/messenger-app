{
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "plugins": ["react"],
  "extends": ["plugin:react/recommended", "plugin:prettier/recommended"],
  "rules": {
    "semi": "off",
    "react/prop-types": 0
  }
}
